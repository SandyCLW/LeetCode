class Solution(object):
    def isPalindrome(self, s):
        """
        :type s: str
        :rtype: bool
        """
#         tmp=str()
#         for c in s: 
#             if c.isalnum():
#                 tmp= tmp + "".join(c.lower())  #str using + perpormance is bad
               
#         print(tmp)
#         print(tmp[::-1])
#         return tmp == tmp[::-1]
        
       
        tmp=[]
        for c in s: 
            if c.isalnum():
                tmp.append(c.lower()) 
        
               
        print(tmp)
        print(tmp[::-1])
        return tmp == tmp[::-1]
        
